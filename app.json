{
  "expo": {
    "name": "ChefIQ Studio",
    "slug": "chefiq-studio",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/studioiq-applogo.png",
    "scheme": "chefiq",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/studioiq-splashscreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#262b2a"
    },
    "androidStatusBar": {
      "backgroundColor": "#0F0F0F",
      "barStyle": "light-content"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chefiq.studio",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.chefiq.studio",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ],
      "softwareKeyboardLayoutMode": "resize"
    },
  "expo": {
    "web": { "bundler": "metro" }
  },

    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos to set cover images for recipes."
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "69655273-c48f-46b2-9756-0355f4aab8a5"
      }
    }
  }
}
